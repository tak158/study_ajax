<div class="page-header">
  <h1>カテゴリー</h1>
</div>
<table class="table table-striped">
  <thead>
    <tr>
      <th>カテゴリー</th>
    </tr>
  </thead>
  <tbody>
    <% @categories.each do |category| %>
      <tr>
        <td><%= link_to category.name, category_books_path(category.id) %></td>
      </tr>
    <% end %>
  </tbody>
</table>
